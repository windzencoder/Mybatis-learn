<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>Message</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>ListServlet</servlet-name>
		<servlet-class>com.imooc.servelt.ListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListServlet</servlet-name>
		<url-pattern>/List.action</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DeleteOneServlet</servlet-name>
		<servlet-class>com.imooc.servelt.DeleteOneServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteOneServlet</servlet-name>
		<url-pattern>/DeleteOne.action</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DeleteBacthServlet</servlet-name>
		<servlet-class>com.imooc.servelt.DeleteBatchServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteBacthServlet</servlet-name>
		<url-pattern>/DeleteBatch.action</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>InitTalkServlet</servlet-name>
		<servlet-class>com.imooc.servelt.InitTalkServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InitTalkServlet</servlet-name>
		<url-pattern>/InitTalkServlet.action</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AutoReplyServlet</servlet-name>
		<servlet-class>com.imooc.servelt.AutoReplyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AutoReplyServlet</servlet-name>
		<url-pattern>/AutoReplyServlet.action</url-pattern>
	</servlet-mapping>
	

</web-app>